generator client {
  provider = "prisma-client-js"
}

datasource db {
 provider  = "postgresql"
  url  	    = env("DATABASE_URL")
  directUrl = env("DATABASE_URL_UNPOOLED")
}



model User {
  userId Int    @id @unique
  email  String @unique
  name   String
  music  Music[] 
}

model Music {
  id         Int      @id @default(autoincrement()) 
  userId     Int      
  url        String   
  isFavourite Boolean 
  user       User     @relation(fields: [userId], references: [userId]) 

  @@unique([userId, url]) 
}